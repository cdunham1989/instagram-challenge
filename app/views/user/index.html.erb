<h1>Instagram Feed</h1>

<h2>Here are all your posts!</h1>

<table>
  <tr>
    <th>Caption</th>
    <th>Image</th>
    <th colspan="2"></th>
  </tr>

  <% @posts.each do |post| %>
    <tr>
      <td><%= post.caption %></td>
      <td><%= post.image %></td>
      <td><%= link_to 'Show', post_path(post) %></td>
    </tr>
  <% end %>
</table>
